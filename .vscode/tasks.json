{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build_c",
        "type": "shell",
        "command": "gcc",
        "args": [
          "-g",
          "*.c",
          "-o",
          "${fileDirname}\\main.exe"
        ],
        "options": {
          "cwd": "${fileDirname}"
        },
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": [
          "$gcc"
        ]
      }

    ]
}